{#
/**
 * @file
 * Description list template.
 */
#}

{% if items %}
  <dl>
    {% for item in items %}
      {% if item.term %}
        <dt>{{ item.term }}</dt>
      {% endif %}
      {% if item.description %}
        <dd>{{ item.description }}</dd>
      {% endif %}
    {% endfor %}
  </dl>
{% endif %}
